div(class="panel panel-default")
	div(class="panel-heading") #{title}
	div(class=panel-body)
		table(class="table")
			tr
				th Mensajes
				th Lista
				th deconectado
			tr
				td
					ul#mensajes
				td
					ul#lista
				td
					ul#desconectado
	div(class="panel-footer")
		input(type="text", class="form-control", placeholder="Mensaje aqui" id="menvio")